.main-page {
  min-height: 100vh;
  padding: 48rpx 32rpx;
  box-sizing: border-box;
  background: #f7f8fa;
}
.title {
  font-size: 36rpx;
  font-weight: 600;
  color: #323233;
  margin-bottom: 16rpx;
}
.desc {
  font-size: 28rpx;
  color: #646566;
}
.content {
  margin-top: 24rpx;
  padding: 24rpx;
  background: #ffffff;
  border-radius: 12rpx;
  color: #323233;
  word-break: break-all;
}
.actions {
  margin-top: 32rpx;
}

